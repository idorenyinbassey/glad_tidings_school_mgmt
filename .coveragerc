[run]
source = .
omit = 
    */migrations/*
    */tests/*
    */.venv/*
    */env/*
    */venv/*
    */__init__.py
    */settings*.py
    */wsgi.py
    */asgi.py
    */manage.py
    */apps.py
    */admin.py
    */urls.py
    */conftest.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError
    except ImportError:
    
[html]
directory = htmlcov
